<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:phone="http://schemas.android.com/apk/res/com.android.phone"
    android:title="@string/wifi_more" >
    
    <PreferenceCategory
        android:key="sim_category_key0"
        android:persistent="false"
        android:title="@string/sub_1" >
        <PreferenceScreen
            android:key="button_cf_expand_key0"
            android:persistent="false"
            android:title="@string/call_forward_title" >
            <intent
                android:action="android.intent.action.MAIN"
                android:targetClass="com.android.phone.GsmUmtsCallForwardOptions"
                android:targetPackage="com.android.phone" />
        </PreferenceScreen>
        <PreferenceScreen
            android:key="button_more_expand_key0"
            android:persistent="false"
            android:title="@string/labelCW" >
            <intent
                android:action="android.intent.action.MAIN"
                android:targetClass="com.android.phone.GsmUmtsAdditionalCallOptions"
                android:targetPackage="com.android.phone" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory
        android:key="sim_category_key1"
        android:persistent="false"
        android:title="@string/sub_2" >
        <PreferenceScreen
            android:key="button_cf_expand_key1"
            android:persistent="false"
            android:title="@string/call_forward_title" >
            <intent
                android:action="android.intent.action.MAIN"
                android:targetClass="com.android.phone.GsmUmtsCallForwardOptions"
                android:targetPackage="com.android.phone" />
        </PreferenceScreen>
        <PreferenceScreen
            android:key="button_more_expand_key1"
            android:persistent="false"
            android:title="@string/labelCW" >
            <intent
                android:action="android.intent.action.MAIN"
                android:targetClass="com.android.phone.GsmUmtsAdditionalCallOptions"
                android:targetPackage="com.android.phone" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory
        android:persistent="false"
        android:title="@string/other_settings" >
        <PreferenceScreen
            android:key="stk"
            android:persistent="false"
            android:title="@string/stk_title" >
        </PreferenceScreen>
    </PreferenceCategory>

</PreferenceScreen>