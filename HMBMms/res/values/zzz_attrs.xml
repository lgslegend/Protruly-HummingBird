<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FolderTextView">
        <attr name="foldline" format="integer" />
    </declare-styleable>

    <!-- tangyisen 0 is send,1 is recv -->
    <declare-styleable name="ZzzMessageItemTextView">
        <attr name="strtype">
            <enum name="send" value="0" />
            <enum name="recv" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ZzzCircleImageView">
        <!-- 源图片 -->
        <attr name="src" format="reference"></attr>
        <!-- 占位图片 -->
        <attr name="holderDrawable" format="reference"></attr>
        <!-- 圆角大小 -->
        <attr name="radius" format="dimension"></attr>
        <attr name="type">
            <enum name="image" value="0" />
            <enum name="video" value="1" />
            <enum name="audio" value="2" />
            <enum name="vcard" value="2" />
        </attr>
    </declare-styleable>
    <!-- tangyisen -->
    <declare-styleable name="ZzzMaxHeightScrollView">
        <attr name="android:maxHeight" />
    </declare-styleable>

</resources>